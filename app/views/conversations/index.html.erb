<div class="row">
	<div class="well">
		<h1>All Conversations</h1>

		<% @conversations.each do |conversation| %>
			<div>
				<%= link_to conversation.subject, conversation_path(conversation) %>
			</div>
		<% end %>

		<%= link_to "New Conversation", new_conversation_path, class: 'btn btn-primary navbar-btn' %>
		<%= link_to "Back To Profile", user_path(current_user), class: 'btn btn-default navbar-btn' %>
	</div>
</div>